      </material>
    </visual>
    <collision>
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
      <origin xyz="0 0 .025"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>


  <joint name="wrist_plate_to_circuit_support" type="fixed">
    <parent link="wrist_plate"/>
    <child link="circuit_support"/>
    <origin xyz="0 0 0"/>
  </joint>

